{
  "name": "langchain-nextjs-template",
  "version": "0.0.0",
  "private": true,
  "packageManager": "yarn@3.5.1",
  "scripts": {
    "dev": "next dev --turbo",
    "dev:webpack": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"app\" \"lib\" \"components\"",
    "test": "node comprehensive-test-runner.js",
    "test:local": "node test-local-setup.js",
    "test:api": "node test-runner.js",
    "test:comprehensive": "node comprehensive-test-runner.js",
    "test:playwright": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
    "test:mcp": "playwright test tests/mcp",
    "test:mcp-api": "playwright test tests/mcp/api",
    "test:mcp-ui": "playwright test tests/mcp/ui",
    "test:llm": "playwright test tests/llm",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install",
    "test:autonomous": "./run-autonomous-tests.sh",
    "test:autonomous-suite": "playwright test tests/autonomous/mcp-autonomous-suite.spec.ts",
    "test:wcag": "playwright test tests/autonomous/mcp-autonomous-suite.spec.ts --project=accessibility-testing",
    "test:multi-device": "playwright test tests/autonomous/mcp-autonomous-suite.spec.ts --project=chromium-desktop --project=firefox-desktop --project=tablet-landscape --project=mobile-android",
    "test:performance": "playwright test tests/autonomous/mcp-autonomous-suite.spec.ts --project=chromium-desktop --trace=on",
    "test:visual": "playwright test tests/autonomous/mcp-autonomous-suite.spec.ts --update-snapshots",
    "test:coverage": "playwright test --coverage",
    "test:parallel": "playwright test --workers=4",
    "test:ci": "playwright test --reporter=github",
    "test:rag-mcp": "node validate-rag-mcp-integration.js",
    "validate:autonomous": "./validate-autonomous-tests.sh",
    "test:local-full": "./run-local-tests.sh",
    "storybook": "storybook dev -p 6006 --quiet",
    "storybook:lazy": "storybook dev -p 6006 --quiet --no-open --lazy-compilation",
    "build-storybook": "storybook build --quiet",
    "build-storybook:lazy": "storybook build --quiet --webpack-stats-json",
    "test:comprehensive-platform": "node comprehensive-platform-test.js",
    "test:openrouter": "node comprehensive-platform-test.js",
    "analyze": "ANALYZE=true npm run build",
    "analyze:lazy": "ANALYZE=true npm run build && npx webpack-bundle-analyzer .next/static/chunks/*.js"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@langchain/anthropic": "^0.3.26",
    "@langchain/community": "^0.3.37",
    "@langchain/core": "^0.3.58",
    "@langchain/google-genai": "^0.2.16",
    "@langchain/langgraph": "^0.2.57",
    "@langchain/langgraph-sdk": "^0.0.60",
    "@langchain/ollama": "^0.2.3",
    "@langchain/openai": "^0.4.9",
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.4",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.1.1",
    "@supabase/supabase-js": "^2.32.0",
    "@types/node": "20.12.12",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "ai": "^3.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "langchain": "^0.3.19",
    "lucide-react": "^0.473.0",
    "next": "^15.2.4",
    "next-themes": "^0.4.4",
    "nuqs": "^2.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-toastify": "^9.1.3",
    "sonner": "^1.7.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "use-stick-to-bottom": "^1.0.44",
    "vaul": "^1.1.2",
    "zod": "^3.23.8",
    "zod-to-json-schema": "^3.23.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^13.4.19",
    "@playwright/test": "^1.55.0",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-docs": "^9.1.3",
    "@storybook/addon-onboarding": "^9.1.3",
    "@storybook/addon-viewport": "^9.0.8",
    "@storybook/nextjs": "^9.1.3",
    "autoprefixer": "10.4.14",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "playwright": "^1.55.0",
    "postcss": "8.4.27",
    "prettier": "^3.4.2",
    "storybook": "^9.1.3",
    "tailwindcss": "3.3.3",
    "typescript": "5.1.6"
  }
}
